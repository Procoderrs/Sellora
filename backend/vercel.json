{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.js"
    }
  ],
  "functions": {
    "api/**/*.js": {
      "includeFiles": "node_modules/cloudinary/lib/utils/analytics/**"
    }
  }
}
