{
  "version": 2,

  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/index.js"
    }
  ],
  "functions": {
    "/**/*.js": {
      "includeFiles": "node_modules/cloudinary/lib/utils/analytics/**"
    }
  }
}


