{
  
  "version": 3,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.js"
    }
  ],


  
  "functions": {
    "/**/*.js": {
      "includeFiles": "node_modules/cloudinary/lib/utils/analytics/**"
    }
  }
}


